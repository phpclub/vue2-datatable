{
  "name": "vue2-datatable-component",
  "description": "The best Datatable for Vue.js 2.x which never sucks",
  "version": "2.1.4",
  "main": "dist/min.js",
  "module": "src/index.js",
  "author": "Ken Berkeley <kenberkeley@foxmail.com>",
  "files": [
    "dist",
    "locale",
    "src"
  ],
  "scripts": {
    "start": "vue build --config build/examples.js",
    "build": "npm run build:examples && npm run build:lib",
    "build:examples": "vue build --config build/examples.js --prod",
    "build:lib": "vue build --config build/lib.js --prod",
    "doc": "docsify serve doc",
    "lint": "eslint -f node_modules/eslint-friendly-formatter build/** examples/src/** locale/** src/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OneWayTech/vue2-datatable.git"
  },
  "keywords": [
    "vue",
    "datatable",
    "table"
  ],
  "dependencies": {
    "lodash": "^4.17.4",
    "replace-with": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "avoriaz": "^4.1.0",
    "babel-polyfill": "^6.23.0",
    "clean-webpack-plugin": "^0.1.16",
    "docsify-cli": "^4.1.10",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.13.0",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.18.1",
    "vue-cli": "^2.8.2"
  },
  "license": "MIT"
}
